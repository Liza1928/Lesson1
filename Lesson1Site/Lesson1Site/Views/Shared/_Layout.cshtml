<html>
<head>
    @await Html.PartialAsync("MetatagsPartial")
    @await Html.PartialAsync("CssPartial")
</head>
<body class="no-sidebar is-preload">
    <div id="page-wrapper">

        <!-- Header Wrapper -->
        @await Html.PartialAsync("HeaderPartial")

        <!-- Main Wrapper -->
        <div id="main-wrapper">
            <div class="wrapper style2">
                <div class="inner">
                    <div class="container">
                       
                    </div>
                </div>
            </div>
            <div class="wrapper style3">
                <div class="inner">
                    <div class="container">
                        <div class="row">
                            <div class="col-8 col-12-medium">
                                @RenderBody()

                                @await Html.PartialAsync("SidebarPartial")
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- Footer Wrapper -->
        @await Html.PartialAsync("FooterPartial")
        <!-- Scripts -->
        @await Html.PartialAsync("ScriptsPartial")
</body>
</html>